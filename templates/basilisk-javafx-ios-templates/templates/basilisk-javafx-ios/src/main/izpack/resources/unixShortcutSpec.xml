<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>

<shortcuts>
    <programGroup defaultName="@{app.title}-@app.version@" location="applications"/>

    <shortcut
        name="@{app.title} @app.version@ Uninstaller"
        programGroup="yes"
        desktop="no"
        applications="no"
        startMenu="no"
        startup="no"
        target="/usr/bin/java"
        commandLine="-jar &quot;$INSTALL_PATH/Uninstaller/uninstaller.jar&quot;"
        initialState="noShow"
        iconFile="trashcan_full"
        iconIndex="0"
        workingDirectory=""
        type="Application"
        encoding="UTF-8"
        KdeSubstUID="true"
        KdeUsername="root"
        createForAll="false"
        Categories="Application;Development;"
        description="This uninstalls @{app.title} @app.version@">
    </shortcut>
</shortcuts>
